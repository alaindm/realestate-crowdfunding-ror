<!-- Show the option to upload a document if the document is not in the database -->
<% unless documents_ready? %>
<div class="ui warning message">
  <div class="header">Mensagens do administrador Imobank:</div>
  Não se esqueça de anexar seus documentos digitalizados
</div>
<%= link_to "CPF", @user.cpf_file.url if @user.cpf_file? %>
<% end %>

