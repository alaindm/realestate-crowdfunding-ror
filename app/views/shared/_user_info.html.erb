<!-- Basic user data -->
<% if logged_in? %>
  <%= @user.name %> , <%= @user.email %>, <%= @user.cpf_file? %>,<!-- CPF, status da conta, -->
  <%= link_to "Atualizar Dados", edit_user_path(current_user), class:"item" %>
  <%= link_to "Log out", logout_path, method: "delete", class:"item" %>
<% else %>
  <%= link_to "Log in", login_path %>
<% end %>





